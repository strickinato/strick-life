<!DOCTYPE html>
<html>
<head>
  <title>StrickLife</title>
  <%= javascript_include_tag "http://maps.googleapis.com/maps/api/js?sensor=true&v=3.exp&libraries=places" %>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
  <div class="alert alert-dismissable alert-danger"><%= error %></div>
  <% end %>
<% end %>

<%= yield %>

</body>
</html>
